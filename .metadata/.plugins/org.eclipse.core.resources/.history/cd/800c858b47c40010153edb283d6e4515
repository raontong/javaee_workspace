package com.ch.site1118.controller;

public class stringtest {
	public static void main(String[] args) {
		String str1="korea";
		String str2="korea";
		System.out.println(str1==str2);  //true
		
		String s1=new "korea";
		String s2=new "korea";
		System.out.println(str1==str2);  //false
		
		
	}
}
